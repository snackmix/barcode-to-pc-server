{
  "name": "barcode-to-pc-server",
  "version": "3.19.0",
  "license": "MIT",
  "main": "bin/main.js",
  "scripts": {
    "start": "gulp start",
    "build": "gulp build",
    "publish": "gulp publish",
    "tsc": "tsc",
    "electron": "electron .",
    "postinstall": "electron-builder install-app-deps && gulp prepare"
  },
  "private": true,
  "build": {
    "files": [
      "!./**/*",
      "!.env",
      "!src"
    ],
    "compression": "maximum",
    "appId": "com.barcodetopc.server",
    "fileAssociations": [
      {
        "ext": "btpt",
        "name": "Barcode to PC Output Template",
        "description": "Barcode to PC Output Template",
        "role": "Editor"
      }
    ],
    "productName": "Barcode to PC server",
    "afterSign": "../electron/notarize.js",
    "dmg": {
      "icon": "electron-resources/dmg-icon.icns",
      "background": "electron-resources/background.tiff",
      "sign": false,
      "contents": [
        {
          "x": 127,
          "y": 250
        },
        {
          "x": 404,
          "y": 250,
          "type": "link",
          "path": "/Applications"
        }
      ],
      "window": {
        "width": 542,
        "height": 430
      }
    },
    "win": {
      "certificateSha1": "EDEB0B36E791F01F813F36B92EA335C860AC3838",
      "target": [
        {
          "target": "nsis-web",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "icon": "electron-resources/icon.ico"
    },
    "linux": {
      "target": "AppImage",
      "artifactName": "${name}.v${version}.${os}.${arch}.${ext}",
      "category": "Utility",
      "desktop": {
        "Name": "${name}",
        "Comment": "${productname}. Please wait...",
        "Terminal": "false"
      }
    },
    "nsisWeb": {
      "oneClick": false,
      "perMachine": true,
      "installerSidebar": "electron-resources/MUI_WELCOMEFINISHPAGE_BITMAP.bmp",
      "shortcutName": "Barcode to PC server",
      "artifactName": "${name}.v${version}.${os}.setup.${ext}"
    }
  },
  "dependencies": {
    "@electron/remote": "2.0.8",
    "@types/gulp": "4.0.9",
    "@woocommerce/woocommerce-rest-api": "1.0.1",
    "axios": "0.26.1",
    "axios-oauth-1.0a": "0.3.4",
    "bonjour": "3.5.0",
    "csv-parse": "4.10.1",
    "csv-stringify": "5.6.2",
    "electron-store": "8.0.1",
    "electron-updater": "5.0.0",
    "https": "1.0.0",
    "mdns": "2.5.1",
    "network": "0.4.1",
    "node-machine-id": "1.1.12",
    "papaparse": "4.4.0",
    "requirejs": "2.3.5",
    "robotjs": "0.6.0",
    "rxjs": "5.5.6",
    "semver": "5.7.1",
    "supplant": "0.2.0",
    "systemjs": "0.21.0",
    "uuid": "8.3.0",
    "ws": "4.1.0"
  },
  "devDependencies": {
    "electron": "14.2.7",
    "electron-builder": "23.0.2",
    "electron-notarize": "1.1.1",
    "fs-extra": "10.0.1",
    "fttx-gulp-github-release": "1.2.1",
    "gulp": "4.0.2",
    "gulp-clean": "0.4.0",
    "gulp-rm": "2.0.0",
    "gulp-sourcemaps": "3.0.0",
    "gulp-spawn": "1.0.0",
    "gulp-zip": "5.1.0",
    "iconv-lite": "0.5.1",
    "mocha": "5.2.0",
    "npx": "10.2.0",
    "spectron": "3.8.0",
    "ts-node": "10.7.0",
    "typescript": "4.6.2"
  }
}
